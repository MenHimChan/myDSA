{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Matrix: g++ build all files",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-I${workspaceFolder}/Matrix",
                "${workspaceFolder}/Matrix/ADT.cpp",
                "${workspaceFolder}/Matrix/main.cpp",
                "-o",
                "${workspaceFolder}/Matrix/program"
            ],
            "options": {
                "cwd": "${workspaceFolder}/Matrix"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Build Matrix program"
